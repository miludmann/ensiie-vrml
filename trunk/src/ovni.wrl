#VRML V2.0 utf8

Shape {
  appearance Appearance {
    material Material {
	    diffuseColor	.8 .8 .8
	    specularColor	.5 .5 .5
	    shininess		0.2
    } # end material
  } # end appearance

  geometry Sphere {
    radius 1
  } # end geometry
} # end shape

#spine
Shape {
  geometry Cylinder {
    radius .1
    height 4
  }
}

#a flat green shape around center
Shape {
  appearance Appearance {
    material Material {
	    diffuseColor	.8 .8 .8
	    specularColor	.5 .5 .5
	    shininess		0.2
    } # end mat
  } # end app
  geometry Cylinder {
    radius 2
    height .2
  } # end geom
} # end shape

# a blue cone
Shape {
  appearance Appearance {
    material Material {
	    diffuseColor	.8 .8 .8
	    specularColor	.5 .5 .5
	    shininess		0.2
    } # end mat
  } # end app
    
  geometry Cone {
    bottomRadius .5
    height 4
  }
}

# with a red box to add corners
Shape {
  appearance Appearance {
    material Material {
	    diffuseColor	.8 .8 .8
	    specularColor	.5 .5 .5
	    shininess		0.2
    } # end mat
  } # end app

  geometry Box {
    size 1.5 1.5 1.5
  }
}












