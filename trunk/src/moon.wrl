#VRML V2.0 utf8

##
# Construction de la lune qui gravite autour de la planete Terre
##
DEF M Transform {
	translation 350 0 0
	
			children [
Shape {
		appearance Appearance {
			material DEF Sun Material {
				diffuseColor .3 .3 .3
				emissiveColor 0 0 0
				
			}
			texture ImageTexture {
			url "../textures/lune.jpg"
			}
			textureTransform TextureTransform {
					scale 8 4
				}
		}
		geometry Sphere {
			
			radius 25.00
		}
		}
]
}
